{
  "name": "Duran Clothing Compositor",
  "description": "Find bodies, add clothes!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "extension_js/background.js"
  },
  "action": {
    "default_popup": "extension_js/popup.html",
    "default_icon": {
      "16": "extension_js/images/get_started16.png",
      "32": "extension_js/images/get_started32.png",
      "48": "extension_js/images/get_started48.png",
      "128": "extension_js/images/get_started128.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
    "16": "extension_js/images/get_started16.png",
    "32": "extension_js/images/get_started32.png",
    "48": "extension_js/images/get_started48.png",
    "128": "extension_js/images/get_started128.png"
  }
}
