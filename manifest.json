{
  "name": "Duran Clothing Compositor",
  "description": "Find bodies, add clothes!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "dist/popup.html",
    "default_icon": {
      "16": "public/icons/get_started16.png",
      "32": "public/icons/get_started32.png",
      "48": "public/icons/get_started48.png",
      "128": "public/icons/get_started128.png"
    }
  },
  "permissions": ["activeTab", "scripting", "desktopCapture"],
  "icons": {
    "16": "public/icons/get_started16.png",
    "32": "public/icons/get_started32.png",
    "48": "public/icons/get_started48.png",
    "128": "public/icons/get_started128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
